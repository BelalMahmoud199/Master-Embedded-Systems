#include <stdio.h>
#include <stdlib.h>

#include <stdio.h>
#include <stdlib.h>
void main ()
{
	int i=0, j;
	for (i=0; i<3 ;i++)
	{
		for (j=0; j<3 ;j++)
		{
			printf("%d ",j+1);
		}
		printf(" \n");

	}
}
